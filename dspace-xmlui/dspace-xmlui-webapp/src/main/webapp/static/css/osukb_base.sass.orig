/* Breadcrumb Styles
 *bds: from the jQuery add-on for magic shrinking breadcrumbs
 *---------------------------------------------------------------------

.breadCrumb
  margin: 0
  padding: 0
  float: left
  display: block
  height: 25px
  overflow: hidden
  width: 780px
  padding: 5px
  ul
    margin: 0
    padding: 0
    height: 21px
    display: block
    li
      white-space: nowrap
      display: block
      float: left
      position: relative
      height: 21px
      overflow: hidden
      line-height: 21px
      margin: 0px 6px 0px 0
      padding: 0px 10px 0px 0
      font-size: .8em
      background: url(../images/Chevron.gif) no-repeat 100% 0
      div.chevronOverlay
        position: absolute
        right: 0
        top: 0
        z-index: 2
      span
        display: block
        overflow: hidden
      a
        display: block
        position: relative
        height: 21px
        line-height: 21px
        overflow: hidden
        float: left
      &.last
        background: none
        margin-right: 0
        padding-right: 0

